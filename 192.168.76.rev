; BIND reverse data file for empty rfc1918 zone
$TTL	86400
@	IN	SOA	servidor-primari.asix2.informatica.com. root.asix2.informatica.com. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			  86400 )	; Negative Cache TTL
;
@	  IN	NS	servidor-primari.
@	  IN	NS	servidor-secundari.
1	  IN	PTR	asix2.informatica.com.
2	  IN	PTR	asix2.informatica.com.
1	  IN	PTR	servidor-primari
2	  IN	PTR	servidor-secundari
10	IN	PTR	client-linux
